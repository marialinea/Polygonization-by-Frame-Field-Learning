{
  "root_dirname": "autokart_dataset",
  "pre_process": true,
  "gt_source": "disk",
  "gt_type": "geojson",
  "gt_dirname": "ground_truth/geojson",  // gt_polygonized are annotations that are the polygonized version of the original gt masks on inria dataset
  "mask_only": false,  // This is the default value to not use this option
  "small": false,
  "data_patch_size": 725,
  "input_patch_size": 512,
  "processed_dirname": "processed_inria_512x512",
  "freeze_backbone": false, // Freezes the backbone parameters when true
  "seed": 0,  // Change this to change the random splitting of data in train/val/test
  "train_fraction": 0.75  // Remaining is validation
}
